{
  "testResultsProcessor": "jest-stare",
  "reporters": ["default"],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!src/index.js",
    "!src/servers/createServer.js"
  ],
  "coverageDirectory": "reports/latest/coverage",
  "coverageReporters": ["json", "html"],
  "transformIgnorePatterns": ["<rootDir>/node_modules/(?!query-builder)"],
  "moduleNameMapper": {
    "^~/(.*)$": "<rootDir>/src/$1",
    "^@/(.*)$": "<rootDir>/lib/$1"
  }
}
